    <!--根据userCode查询-->
    <select id="getByUserCode" parameterType="Map"
            resultType="User">
     SELECT
        <include refid="Field"></include>
        FROM
        <include refid="tableName"></include>
        WHERE USER_CODE= #{usercode}
    </select>

    <!-- 字段值 -->
    <sql id="FieldValue">
				#{userId},
				#{userCode},
				#{userName},
				#{userNameEn},
				#{userNamePy},
				#{userNameFr},
				#{orgCode},
				#{postNo},
				#{userSex},
				#{nationality},
				#{ethnic},
				#{nativePlace},
				#{birthPlace},
				#{birthDate},
				#{idDecimal},
				#{photoId},
				#{joinDate},
				#{status},
				#{tellerId},
				#{residentialAddress},
				#{mobilePhone},
				#{remark},
				#{password},
				#{salt},

    <!-- 系统管理->系统管理-用户表-分页查询列信息 -->
    <select id="findForPageList" parameterType="Page"
            resultType="User">
        SELECT
        <include refid="Field"></include>
        FROM
        <include refid="tableName"></include>
        WHERE 1=1
        <if test="variables.userId != null and variables.userId != ''">
            AND USER_ID
            <choose>
                <when
                        test="operators.userId != null and operators.userId != '' ">${operators.userId}
                </when>
                <otherwise>=</otherwise>
            </choose>
            #{variables.userId}
        </if>
        <if test="variables.userCode != null and variables.userCode != ''">
            AND USER_CODE

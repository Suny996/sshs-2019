				MOBILE_PHONE,
				REMARK,
				PASSWORD,
				SALT,
				ONLINE_STATUS,
				IP_ADDR,
				USER_THEME,
				PD_COUNT,
				PD_MODTIME,
				PD_LOCKTIME,

            AND BIRTH_PLACE = #{variables.birthPlace}
        </if>
        <if test="variables.birthDate != null and variables.birthDate != ''">
            AND BIRTH_DATE = #{variables.birthDate}
        </if>
        <if test="variables.idDecimal != null and variables.idDecimal != ''">

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sshs.system.user.mapper.UserMapper">
    <!--表名 -->
    <sql id="tableName">
		SYS_USER
	</sql>


    <!-- 字段 -->
    <sql id="Field">
				USER_ID,
				USER_CODE,
				USER_NAME,
				USER_NAME_EN,
				USER_NAME_PY,
				USER_NAME_FR,
				ORG_CODE,
				POST_NO,
				USER_SEX,
				NATIONALITY,
				ETHNIC,
